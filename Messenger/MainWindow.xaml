<Window x:Class="Messenger.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Messenger"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="1310" ResizeMode="NoResize" WindowStyle="None">
    <Border BorderBrush="Silver" BorderThickness="1">
    <Grid Background="#1f2326" MouseLeftButtonDown="MouseLeftButtonDown">
        <Image Source="/Images/bg.jpg" Opacity="0.1"/>

        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden"/>

    </Grid>
    </Border>
</Window>
